<div class="container">
  <h1>Edit your profile</h1>
  <%= simple_form_for current_user do |f|%>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :photo, as: :attachinary %>
    <%= f.submit "Save your changes", class: "btn btn-primary" %>
  <% end %>
</div>
